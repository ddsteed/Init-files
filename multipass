u22=192.168.64.16

mln () {
  multipass launch --name $1 -c 4 -m 8G -d 50G
}

shm () {
  ssh -X -Y f1@$u22
}

ms () {
  multipass shell u22
}

mstop () {
  multipass stop u22
}

mst () {
  multipass start u22
}

mrs () {
  multipass restart u22
}

min () {
  multipass info u22
}

alias mls='multipass ls'
alias mf='multipass find'

mm () {
    multipass mount $1 $2
}

mfengh () {
  mm /Users/fengh u22:/Users/fengh
}
